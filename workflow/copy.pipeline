pipeline {
    agent any

    stages {
        stage('Checkout') { // добавим новый Stage
            steps {
                git branch: 'main', url: "git@github.com:kaurych/Pipeline-Ansible.git", credentialsId: 'github-ssh-key' 
            }
        }
        stage('Deploy') {
            steps {
                   sh 'ansible-playbook playbook_rsync.yml'
                }
            }
        }
   }
}
